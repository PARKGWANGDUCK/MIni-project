package miniProject.getLunch;

public class Todo {

	private int id; // 메뉴에 할당된 id
	private String title; // 한식 중식 일식 등
	private String menu; // 메뉴명

	public Todo(int id, String title, String menu) {
		this.id = id;
		this.title = title;
		this.menu = menu;
	}
	
	public Todo(int id) {
		super();
		this.id = id;
	}

	public Todo(String title) {
		super();
		this.title = title;
	}
	
	public Todo(int id, String title) {
		super();
		this.id = id;
		this.title = title;
	}

	public int getId() {
		return id;
	}

	public String getTitle() {
		return title;
	}

	public String getMenu() {
		return menu;
	}


}
